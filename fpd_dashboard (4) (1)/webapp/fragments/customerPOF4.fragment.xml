<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>customerPo}"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		supportRanges="true"
		key="Bstkd"
		descriptionKey="Bstkd">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Bstkd"
						label="{i18n>customerPO}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Bstkd" />
						</fb:control>
					</fb:FilterGroupItem>
					<!-- <fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Name"
						label="Name"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Name" />
						</fb:control>
					</fb:FilterGroupItem> -->
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
