<mvc:View
    controllerName="fdpdashboard.controller.Dashboard"
    height="100%"
    xmlns:mvc="sap.ui.core.mvc" 	xmlns:ui="sap.ui.table"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:svm="sap.ui.comp.smartvariants"
>
    <f:DynamicPage headerExpanded="{/headerExpanded}">
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <!-- <Title text="{i18n>appTitle}" /> -->
                </f:heading>
                <f:expandedContent>
                    <svm:SmartVariantManagement
                        id="svm"
                        class="sapUiSmallMarginBegin"
                        showExecuteOnSelection="true"
                    />
                </f:expandedContent>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader>
                <f:content>
                    <fb:FilterBar
                        id="mainPagefilterbar"
                        persistencyKey="myPersKey"
                        useToolbar="false"
                        search=".onSearch"
                        filterChange=".onFilterChange"
                        afterVariantLoad=".onAfterVariantLoad"
                    >
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem
                                name="Sales Org"
                                label="{i18n>salesOrg}"
                                groupName="Group1"
                                visibleInFilterBar="true" 
                            >
                                <fb:control>
                                    <MultiInput
                                        id="salesOrgInput"
                                        value="{FiltersModel>/SalesOrg}"
                                        name="{i18n>salesOrg}"
                                        change="onSelectionChange"
                                        valueHelpRequest=".handleSalesOrgF4" valueHelpOnly="true"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Order Type"
                                label="{i18n>orderType}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="orderTypeInput"
                                        value="{FiltersModel>/OrderType}"
                                        name="Order Type"
                                        valueHelpRequest=".handleOrderTypeF4"  valueHelpOnly="true"
                                        selectionChange=".onSelectionChange"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Order Creation Date"
                                label="{i18n>orderCreatDate}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <DateRangeSelection
                                        required="true"
                                        dateValue="{FiltersModel>/OrderCreateDateFrom}"
                                        secondDateValue="{FiltersModel>/OrderCreateDateTo}"
                                        id="orderCreationDate"
                                        displayFormat="dd.MM.yyyy"
                                        change="handleChange"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Division"
                                label="{i18n>division}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="divisionInput"
                                        value="{FiltersModel>/Division}"
                                        name="Division"
                                        valueHelpRequest=".handleDivisionF4"  valueHelpOnly="true"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                              <fb:FilterGroupItem
                                name="Sales Office"
                                label="{i18n>salesOffice}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="salesOfficeInput"
                                        value="{FiltersModel>/SalesOffice}"
                                        name="Sales Office"
                                        valueHelpRequest=".handleSalesOfficeF4"  valueHelpOnly="true"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Sales Group"
                                label="{i18n>salesGroup}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="salesGroupInput"
                                        value="{FiltersModel>/SalesGroup}"
                                        name="Sales Office"
                                        valueHelpRequest=".handleSalesGroupF4"  valueHelpOnly="true"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Sold-To"
                                label="{i18n>soldTo}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="soldToInput"
                                        value="{FiltersModel>/SoldTo}"
                                        name="Sold To"
                                        valueHelpRequest=".handleSoldToF4"  valueHelpOnly="true"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Ship-To"
                                label="{i18n>shipTo}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="shipToInput"
                                        value="{FiltersModel>/ShipTo}"
                                        valueHelpRequest=".handleShipToF4"  valueHelpOnly="true"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Sales Order"
                                label="{i18n>salesOrder}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="salesOrderInput"
                                        value="{FiltersModel>/SalesOrderFrom}"
                                        valueHelpRequest=".handleSalesOrderF4"
                                        showValueHelp="true"  valueHelpOnly="true"
                                        selectionChange=".onSelectionChange"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Contract"
                                label="{i18n>contract}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        id="contractInput"
                                        value="{FiltersModel>/Contract}"
                                        valueHelpRequest=".handleContractF4"
                                        showValueHelp="true"  valueHelpOnly="true"
                                        selectionChange=".onSelectionChange"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Customer PO #"
                                label="{i18n>customerPo}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        value="{FiltersModel>/CustomerPo}"
                                        id="customerPOInput"
                                        valueHelpRequest=".handleCustomerPOF4"
                                        showValueHelp="true"  valueHelpOnly="true"
                                        selectionChange=".onSelectionChange"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Plant"
                                label="{i18n>plant}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <MultiInput
                                        value="{FiltersModel>/Plant}"
                                        id="plantInput"
                                        name="Plant"
                                        valueHelpRequest=".handlePlantF4"
                                        showValueHelp="true"  valueHelpOnly="true"
                                        selectionChange=".onSelectionChange"
                                    >
                                        <core:Item
                                            key="{key}"
                                            text="{name}"
                                        />
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Business Scenerio"
                                label="{i18n>busScenerio}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <ComboBox id="busScenerioBox" value="{FiltersModel>/BusScenerio}"
                                      
                                        items="{BSDataModel>/results}"
                                    >
                                        <core:Item
                                            key="{BSDataModel>Low}"
                                            text="{BSDataModel>High}"
                                        />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            
                            <fb:FilterGroupItem
                                name="Ex-Space Indicator"
                                label="{i18n>exSpaceIndicator}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <ComboBox id="exSpaceCombo" value="{FiltersModel>/ExSpaceIndictor}"
                                      
                                        items="{LocalModel>/exSpace}"
                                    >
                                        <core:Item
                                            key="{LocalModel>text}"
                                            text="{LocalModel>text}"
                                        />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Call By Date"
                                label="{i18n>callByDate}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <DateRangeSelection
                                        dateValue="{FiltersModel>/CallByDateFrom}"
                                        secondDateValue="{FiltersModel>/CallByDateTo}"
                                        displayFormat="dd.MM.yyyy"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem
                                name="Priced Date"
                                label="{i18n>pricedDate}"
                                groupName="Group1"
                                visibleInFilterBar="true"
                            >
                                <fb:control>
                                    <DateRangeSelection
                                        dateValue="{FiltersModel>/PricedDateFrom}"
                                        secondDateValue="{FiltersModel>/PricedDateTo}"
                                        displayFormat="dd.MM.yyyy"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem name="Layout" label="{i18n>layout}" groupName="Group1" visibleInFilterBar="true">
									<fb:control>
										<MultiComboBox  
											name="Layout" id="layout" showValueHelp="true" showSelectAll="true"
											 items="{	path: 'LocalModel>/Columns'}" >
											<core:Item key="{LocalModel>ID}" text="{LocalModel>Text}" />
										</MultiComboBox>
									</fb:control>
								</fb:FilterGroupItem>
                        </fb:filterGroupItems>
                    </fb:FilterBar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
        	<ui:Table
				id="table" visible="false" type="Active"
				selectionMode="MultiToggle" showColumnVisibilityMenu="true"
				 rows="{
						path: 'TableDataModel>/results'
					}"
				visibleRowCount="12"
				enableCellFilter="{ui>/cellFilterOn}"
				filter="filterPrice"
				ariaLabelledBy="title" rowActionCount="1" rowSelectionChange="onDetailPress">
				<ui:extension>
					<OverflowToolbar style="Clear">
					  <Title
                            text="Items({= ${TableDataModel>/results}.length})"
                            level="H2"
                        />
                        <ToolbarSpacer />
                        <Button
                            tooltip="Execute"
                            text="{i18n>execute}" press="onExecuteAllSteps"
                        />
                        <!-- Jayesh commented button for GPT 23294 JIRA DEFECT on 5 Jan 2024 -->
                        <!-- <Button
                            tooltip="Move"
                            text="{i18n>moveStocks}" press="onMovetoStocks"
                        /> -->
                         <!-- Jayesh commented button for GPT 23294 JIRA DEFECT on 5 Jan 2024 -->
                        <Button
                            tooltip="STO"
                            text="{i18n>createSto}" press="onCreateSTO"
                        />
                        <Button
                            tooltip="STO Delivery" press="onStoDelivery"
                            text="{i18n>createStoDel}"
                        />
                        <Button
                            tooltip="Update Ex-Space Indicator"
                            text="{i18n>updateExSpacIndic}"
                            press="updateExSpaceIndicator"
                        />
                        <Button
                            tooltip="Update Call-By Date"
                            text="{i18n>updateCalByDate}"
                            press="openUpdateCallByDateDialog"
                        />
                        <!-- Jayesh disabled buttons for GPT 27503 JIRA DEFECT on 5 Jan 2024 -->
                            <Button
                            tooltip="Call for Price"
                            text="{i18n>uCallForPrice}"
                            press="openUpdateCallByPriceDialog" enabled="false"
                        /> 
                        <Button
                            tooltip="Update Final Price (China)"
                            text="{i18n>updateFinalPricChina}"  press="openUpdateCallForPriceChinaDialog" enabled="false"
                        />
                         <!-- Jayesh disabled buttons  for GPT 27503 JIRA DEFECT on 5 Jan 2024 -->

                        <!-- <Button
                            icon="sap-icon://action-settings"
                            press="onPersoButtonPressed"
                        /> -->
                        <Button
                            icon="sap-icon://excel-attachment"
                            press="onExport"
                        />
					</OverflowToolbar>
				</ui:extension>
				<ui:columns>
					<ui:Column
						width="11rem" 
						filterProperty="SalesOrder" id="SalesOrder">
						<Label text="{i18n>salesOrder}" />
						<ui:template>
							<Text text="{TableDataModel>SalesOrder}" wrapping="false" />
