<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
> 
<Dialog
        title="{i18n>blkQty}"
        contentWidth="700px"
        titleAlignment="Center"
    >
        <content>
            <table:Table
                id="blockedDialog"
                rows="{ path: 'blockedModel>/results'}"
                visibleRowCount="10"
                selectionMode="None"
            >
                <table:columns>
                    <table:Column id="blkqty">
                       <Label text="{i18n>blkQty}"/>

                        <table:template>
                               <Text text="{path: 'blockedModel>BlockedQty', formatter: '.formatter.QtyFormatting'}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="contract">
                         <Label text="{i18n>contractNum}"/>
                        <table:template>
                            <Link text="{blockedModel>ContractNumber}" press="onContractNoCrossApp" id="ContractNo"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="reason">
                        <Label text="{i18n>blkreason}"/>
                        <table:template>
                            <Text text="{blockedModel>BlockedReason}" />
                        </table:template>
                    </table:Column> 
                </table:columns>
            </table:Table>
        </content>  
  <endButton>
   <Button text="Cancel" press="executeCuttingLogic" />
  </endButton>
  </Dialog>
</core:FragmentDefinition>