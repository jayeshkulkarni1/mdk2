<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core">
	<Dialog
		id="cuttingLogic"
		title="{i18n>cutingLogic}" contentWidth="550px" contentHeight="420px">
		<content>
		
			
				<f:SimpleForm id="SimpleFormCuttingLogic"
					editable="true"
					layout="ColumnLayout"
					columnsM="2"
					columnsL="2"
					columnsXL="2">
					<f:content>
						<core:Title text="" />
                        <Label text="{i18n>week}" />
						<MultiInput showValueHelp="true"/>
						<Label text="{i18n>accountMng}" /> 
						<Input/>
					</f:content>
				</f:SimpleForm>
				
			
		</content>
		  <beginButton>
   <Button text="Execute" press="executeCuttingLogic" />
  </beginButton>
  <endButton>
   <Button text="Cancel" press="executeCuttingLogic" />
  </endButton>
		
	</Dialog>
</core:FragmentDefinition>


